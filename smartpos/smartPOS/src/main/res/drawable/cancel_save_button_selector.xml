<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="5dp" />
            <solid android:color="@color/return_dish_text_enabled_false" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:color="@color/change_table_menu_text_normal" />
            <solid android:color="@color/order_price_total_bg" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:color="@color/change_table_menu_text_normal" />
            <solid android:color="@color/White" />
        </shape>
    </item>


</selector>